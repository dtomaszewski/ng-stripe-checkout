  "homepage": "https://github.com/westphalen/ng9-stripe-checkout#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "codelyzer": "^5.0.1",
    "copyfiles": "^1.2.0",
    "rollup": "^0.49.3",
    "rxjs": "^6.5.4",
    "typescript": "3.5.3",
